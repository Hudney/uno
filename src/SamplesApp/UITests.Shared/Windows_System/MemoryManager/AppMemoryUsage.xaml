<Page
    x:Class="UITests.Shared.Windows_System.MemoryManager.AppMemoryUsage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:u="using:Uno.UI.Samples.Controls"
	xmlns:ios="http://nventive.com/ios"
	xmlns:win="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:android="http://nventive.com/android"
	xmlns:xamarin="http://uno.ui/xamarin"
	mc:Ignorable="d ios android xamarin"
	d:DesignHeight="2000"
	d:DesignWidth="400">

	<xamarin:Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>

		<StackPanel Background="Honeydew">
			<TextBlock Text="Value AppMemoryUsage" FontWeight="Bold" />
			<TextBlock x:Name="ValueAppMemoryUsage" />
		</StackPanel>
	</xamarin:Grid>
</Page>
